<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>jbpm-form-builder-client-example</display-name>
    <servlet>
        <servlet-name>FormHostServlet</servlet-name>
        <servlet-class>com.salaboy.example.FormHostServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HumanTaskServlet</servlet-name>
        <servlet-class>com.salaboy.example.HumanTaskServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormHostServlet</servlet-name>
        <url-pattern>/FormHostServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HumanTaskServlet</servlet-name>
        <url-pattern>/HumanTaskServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>FormHostServlet</welcome-file>
    </welcome-file-list>
</web-app>
